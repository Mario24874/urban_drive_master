{
  "name": "urbandrive",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:mobile": "tsc -b && vite build && npx cap sync",
    "lint": "eslint .",
    "preview": "vite preview",
    "capacitor:init": "npx cap init",
    "capacitor:add:android": "npx cap add android",
    "capacitor:add:ios": "npx cap add ios",
    "capacitor:sync": "npx cap sync",
    "capacitor:run:android": "npx cap run android",
    "capacitor:run:ios": "npx cap run ios",
    "capacitor:build:android": "npm run build:mobile && npx cap run android",
    "capacitor:build:ios": "npm run build:mobile && npx cap run ios",
    "android:dev": "npm run build:mobile && npx cap run android --livereload --external",
    "android:build": "npm run build:mobile && npx cap build android",
    "ios:dev": "npm run build:mobile && npx cap run ios --livereload --external",
    "ios:build": "npm run build:mobile && npx cap build ios"
  },
  "dependencies": {
    "@capacitor/app": "^6.0.0",
    "@capacitor/device": "^6.0.0",
    "@capacitor/filesystem": "^6.0.3",
    "@capacitor/geolocation": "^6.0.0",
    "@capacitor/haptics": "^6.0.0",
    "@capacitor/network": "^6.0.0",
    "@capacitor/push-notifications": "^6.0.0",
    "@capacitor/share": "^6.0.3",
    "@capacitor/status-bar": "^6.0.0",
    "@capacitor/toast": "^6.0.0",
    "@firebasegen/default-connector": "file:dataconnect-generated/js/default-connector",
    "@supabase/supabase-js": "^2.52.1",
    "@types/mapbox-gl": "^3.4.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "firebase": "^10.14.1",
    "firebase-admin": "^12.7.0",
    "lucide-react": "^0.525.0",
    "mapbox-gl": "^3.7.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2"
  },
  "devDependencies": {
    "@capacitor/android": "^6.0.0",
    "@capacitor/cli": "^6.0.0",
    "@capacitor/core": "^6.0.0",
    "@capacitor/ios": "^6.0.0",
    "@eslint/js": "^9.11.1",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.2",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.11.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "globals": "^15.10.0",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.8.0",
    "vite": "^5.4.8",
    "vite-plugin-pwa": "^0.20.5",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
